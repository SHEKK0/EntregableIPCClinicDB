      <children>
        <GridPane prefHeight="486.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
          <children>
            <TextField fx:id="textNombre" disable="false" prefWidth="200.0" text="textNombre" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nombre" GridPane.columnIndex="0" GridPane.rowIndex="0">
              <font>
                <Font size="18.0" fx:id="x1" />
              </font>
            </Text>
            <ImageView fx:id="imagePersona" fitHeight="80.25" fitWidth="107.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="1">
              <image>
                <Image url="@../images/default.png" />
              </image>
            </ImageView>
            <Text font="$x1" strokeType="OUTSIDE" strokeWidth="0.0" text="Telefono" GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <TextField fx:id="textTel" prefWidth="200.0" text="663682332" GridPane.columnIndex="1" GridPane.rowIndex="2">
              <GridPane.margin>
                <Insets right="250.0" />
              </GridPane.margin>
            </TextField>
            <TableView fx:id="tabCitas" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
              <columns>
                <TableColumn fx:id="colDate" maxWidth="5000.0" minWidth="10.0" prefWidth="196.0" text="Fecha" />
                <TableColumn fx:id="colMed" maxWidth="5000.0" minWidth="10.0" prefWidth="167.0" text="Medico" />
              </columns>
            </TableView>
            <Text font="$x1" strokeType="OUTSIDE" strokeWidth="0.0" text="Citas" GridPane.columnIndex="0" GridPane.rowIndex="4" />
            <Text font="$x1" strokeType="OUTSIDE" strokeWidth="0.0" text="Identificador(DNI) :" GridPane.columnIndex="0" GridPane.rowIndex="3" />
            <TextField fx:id="textId" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
          </children>
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="299.0" minWidth="10.0" prefWidth="238.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="362.0" minWidth="10.0" prefWidth="362.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints maxHeight="76.0" minHeight="10.0" prefHeight="76.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="77.0" minHeight="10.0" prefHeight="77.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="71.0" minHeight="10.0" prefHeight="71.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="153.0" minHeight="10.0" prefHeight="51.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="218.0" minHeight="10.0" prefHeight="201.0" vgrow="SOMETIMES" />
          </rowConstraints>
        </GridPane>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
          <children>
            <Button fx:id="closeButton" contentDisplay="CENTER" font="$x1" mnemonicParsing="false" onAction="#closeButton" text="Cerrar" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin></Button>
                  <Button fx:id="seeDate" mnemonicParsing="false" text="Ver Cita">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="deleteDate" layoutX="320.0" layoutY="47.0" mnemonicParsing="false" text="Borrar Cita">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </Button>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
